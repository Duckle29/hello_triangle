ninja_required_version = 1.5.1

# Rules for compiling.

rule cpp_COMPILER
 command = c++ $ARGS -MD -MQ $out -MF '$DEPFILE' -o $out -c $in
 deps = gcc
 depfile = $DEPFILE
 description = Compiling C++ object $out.

# Rules for linking.

rule cpp_LINKER
 command = c++ $ARGS -o $out $in $LINK_ARGS
 description = Linking target $out.

# Other rules

rule CUSTOM_COMMAND
 command = $COMMAND
 description = $DESC
 restat = 1

rule REGENERATE_BUILD
 command = /home/mikkel/.local/bin/meson --internal regenerate /home/mikkel/Nextcloud/VGIS7/computer_graphics/mm1/hello_triangle /home/mikkel/Nextcloud/VGIS7/computer_graphics/mm1/hello_triangle/build --backend ninja
 description = Regenerating build files.
