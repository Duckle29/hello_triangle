# Phony build target, always out of date

build PHONY: phony 

# Build rules for targets

build main@exe/hellotriangle.cpp.o: cpp_COMPILER ../hellotriangle.cpp
 DEPFILE = main@exe/hellotriangle.cpp.o.d
 ARGS = -Imain@exe -I. -I.. -I/usr/include -I/usr/include/libdrm -fdiagnostics-color=always -pipe -D_FILE_OFFSET_BITS=64 -Wall -Winvalid-pch -Wnon-virtual-dtor -Wextra -Wpedantic -std=c++14 -O2 -g -fpermissive -m32

build main: cpp_LINKER main@exe/hellotriangle.cpp.o | /usr/lib64/libglut.so /usr/lib/libGLEW.so /usr/lib64/libGLU.so /usr/lib64/libGL.so
 LINK_ARGS = -Wl,--no-undefined -Wl,--as-needed -Wl,--start-group /usr/lib64/libglut.so /usr/lib/libGLEW.so /usr/lib64/libGLU.so /usr/lib64/libGL.so -Wl,--end-group

# Test rules

